{
  "extends": ["astro/tsconfigs/strictest", "../tsconfig.json"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@client/*": ["../client/src/*"],
      "@api/*": ["../api/src/*"],
      "~/*": ["./public/*"],
      "panda/*": ["./panda/*"],
      "package": ["./package.json"]
    }
  },
  "exclude": ["node_modules"]
}
