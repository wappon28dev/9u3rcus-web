{
  "name": "tpl-astro",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prepare": "panda codegen && bun run assets:dl",
    "dev": "wrangler pages dev -- bunx astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "assets:dl": "bun run ./scripts/assets-dl/index.ts",
    "generate:cms": "bunx microcms-typescript schemas src/types/cms-types.ts",
    "copy:fn": "bunx cpx \"functions/**/*\" \"./dist/functions\"",
    "preview": "astro preview",
    "astro": "astro",
    "fmt": "prettier --write . --plugin=prettier-plugin-astro",
    "lint": "eslint --ext .ts,.tsx,.astro ./src"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.5",
    "@astrojs/react": "^3.0.10",
    "@astrojs/sitemap": "^3.1.1",
    "@hookform/resolvers": "^3.3.4",
    "@iconify-json/mdi": "^1.1.64",
    "@iconify/react": "^4.1.1",
    "@marsidev/react-turnstile": "^0.5.3",
    "@nanostores/persistent": "^0.10.1",
    "@nanostores/react": "^0.7.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "astro": "^4.4.4",
    "astro-compress": "^2.2.10",
    "astro-embed": "^0.6.1",
    "astro-icon": "^1.1.0",
    "astro-layouts": "^1.0.0",
    "astro-meta-tags": "^0.2.1",
    "astro-seo": "^0.8.2",
    "cheerio": "^1.0.0-rc.12",
    "highlight.js": "^11.9.0",
    "image-size": "^1.1.1",
    "microcms-js-sdk": "^2.7.0",
    "microcms-typescript": "^1.0.14",
    "nanostores": "^0.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-svg": "^16.1.33",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "@pandacss/astro": "^0.25.0",
    "@pandacss/dev": "^0.32.1",
    "@types/bun": "^1.0.7",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "sharp": "^0.33.0"
  }
}
